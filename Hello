import static java.lang.System.out;

public class Hello {
        public static void main( String args[] )
        {
            out.println("Hello World!");
            out.println("My arguments:");
            for (int i = 0; i < args.length; i++)
            {
                out.println(" arg["+i+"] :"+args[i]);
            }
            out.println("Good bye!");
        }
    }
